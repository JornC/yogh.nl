<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with field="res" type="nl.yogh.business.card.client.resources.R.ApplicationResource" />
  <ui:style type="nl.yogh.business.card.client.widget.BlockChainHexMatrix.CustomStyle">
    .container {
    color: #CCC;
    text-shadow: 1px 1px 2px rgba(33, 182, 216, 0.25);
    }
    .container > * {
    transition: all 0.8s ease-out;
    padding: 1px;
    }
    .highlight {
    background-color: #21B6D8;
    color: white;
    }
  </ui:style>
  <g:FlowPanel ui:field="container" styleName="{style.container} {res.css.flex} {res.css.grow} {res.css.wrap} {res.css.spaceAround}" />
</ui:UiBinder>